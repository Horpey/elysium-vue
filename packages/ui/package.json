{
  "name": "@horpey/elysium-vue",
  "type": "module",
  "version": "1.0.7",
  "packageManager": "pnpm@8.10.5",
  "homepage": "https://elysium-vue.vercel.app",
  "repository": "horpey/elysium-vue",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/horpey-elysium-vue.es.js",
      "require": "./dist/horpey-elysium-vue.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/horpey-elysium-vue.umd.js",
  "module": "./dist/horpey-elysium-vue.es.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly && pnpm run build:style",
    "build:style": "postcss src/styles/index.css -o dist/style.css",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.19",
    "@unhead/vue": "1.8.10",
    "@vueuse/core": "10.8.0",
    "dialog-polyfill": "0.5.6",
    "vue": "3.3.8",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.6.4",
    "@iconify-json/carbon": "1.1.30",
    "@iconify-json/ph": "1.1.11",
    "@tailwindcss/forms": "0.5.7",
    "@types/node": "20.11.19",
    "@vitejs/plugin-vue": "4.6.2",
    "@vitest/ui": "1.2.0",
    "@vue-macros/volar": "0.18.11",
    "@vue/test-utils": "2.4.4",
    "autoprefixer": "10.4.17",
    "eslint": "8.56.0",
    "eslint-plugin-tailwindcss": "3.14.3",
    "jsdom": "22.1.0",
    "pnpm": "8.10.5",
    "postcss": "8.4.35",
    "postcss-cli": "^11.0.0",
    "sass": "1.71.1",
    "simple-git-hooks": "2.9.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "unplugin-auto-import": "0.17.5",
    "unplugin-icons": "0.18.5",
    "unplugin-vue-components": "0.26.0",
    "unplugin-vue-macros": "2.7.10",
    "unplugin-vue-router": "0.7.0",
    "vite": "5.1.4",
    "vitest": "1.2.0",
    "vue-tsc": "1.8.27"
  }
}
