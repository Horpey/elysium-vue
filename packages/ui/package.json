{
  "name": "@horpey/elysium-vue",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.10.5",
  "homepage": "https://elysium-vue.vercel.app",
  "repository": "horpey/elysium-vue",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./index.css": {
      "import": "./src/styles/index.css",
      "require": "./src/styles/index.css"
    },
    "./elysiumPreset": {
      "import": "./config/elysiumPreset.ts",
      "require": "./config/elysiumPreset.ts"
    }
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.19",
    "@unhead/vue": "1.8.10",
    "@vueuse/core": "10.8.0",
    "vue": "3.3.8",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.6.4",
    "@iconify-json/carbon": "1.1.30",
    "@iconify-json/ph": "1.1.11",
    "@tailwindcss/forms": "0.5.7",
    "@types/node": "20.11.19",
    "@vitejs/plugin-vue": "4.6.2",
    "@vitest/ui": "1.2.0",
    "@vue-macros/volar": "0.18.11",
    "@vue/test-utils": "2.4.4",
    "autoprefixer": "10.4.17",
    "eslint": "8.54.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "jsdom": "22.1.0",
    "pnpm": "8.10.5",
    "postcss": "8.4.35",
    "sass": "1.71.1",
    "simple-git-hooks": "2.9.0",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.3",
    "unplugin-auto-import": "0.16.7",
    "unplugin-icons": "0.17.4",
    "unplugin-vue-components": "0.25.2",
    "unplugin-vue-macros": "2.7.10",
    "unplugin-vue-router": "0.7.0",
    "vite": "5.0.0",
    "vitest": "1.2.0",
    "vue-tsc": "1.8.27"
  }
}
